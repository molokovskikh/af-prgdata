<?xml version="1.0"?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net"/>
	</configSections>
	<appSettings>
		<add key="MySqlFilePath" value="\\fms.adc.analit.net\AFFiles"/>
		<add key="MySqlLocalFilePath" value="\\fms.adc.analit.net\AFFiles"/>
		<add key="DocumentsPath" value="FtpRoot\"/>
		<add key="MaxGetUserDataSession" value="50"/>
		<add key="ZipProcessorAffinity" value="252"/>
		<add key="ErrorMail" value="kvasovtest@analit.net"/>
		<add key="WaybillServiceUri" value="net.tcp://localhost:901/WaybillService"/>
		<add key="FakeHost" value=""/>
		<add key="FakeIp" value=""/>
		<add key="FakeDNS" value=""/>
		<add key="log4net.Internal.Debug" value="true"/>
		<add key="AccessTimeHistoryDepth" value="5"/>
		<add key="MustLogHttpcontext" value="False"/>
		<add key="TechMail" value="kvasovtest@analit.net"/>
		<add key="BillingMail" value="kvasovtest@analit.net"/>
		<add key="OfficeMail" value="kvasovtest@analit.net"/>
    <add key="SupportMail" value="kvasovtest@analit.net"/>
  </appSettings>

	<connectionStrings>
		<add name="Local" connectionString="Database=usersettings;Data Source=localhost;User Id=root;pooling=true;default command timeout=120;Allow user variables=true;convert zero datetime=yes;"/>
		<add name="integration" connectionString="Database=usersettings;Data Source=testsql.analit.net;User Id=system;Password=newpass;pooling=true;default command timeout=120;Allow user variables=true;convert zero datetime=yes;"/>
  </connectionStrings>

	<system.web>
		<pages validateRequest="false" />
		<httpRuntime executionTimeout="3000" requestValidationMode="2.0" />
		<compilation debug="true" />
		<globalization requestEncoding="windows-1251"/>
		<webServices>
			<protocols>
				<clear/>
				<add name="HttpPost"/>
				<add name="HttpSoap12"/>
				<add name="Documentation"/>
			</protocols>
		</webServices>
	</system.web>
	<log4net>
		<appender name="LogFile" type="log4net.Appender.RollingFileAppender, log4net">
			<lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
			<file value="Logs/PrgData.txt"/>
			<appendToFile value="true"/>
			<rollingStyle value="Size"/>
			<staticLogFileName value="true"/>
			<maximumFileSize value="10MB"/>
			<threshold value="WARN"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%d{dd.MM.yyyy HH:mm:ss.fff} %property{user} [%t] %-5p %c - %m%n"/>
			</layout>
		</appender>
		<appender name="DebugLogFile" type="log4net.Appender.RollingFileAppender, log4net">
			<lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
			<file value="Logs/PrgDataDebug_"/>
			<appendToFile value="true"/>
			<rollingStyle value="Date"/>
			<staticLogFileName value="false"/>
			<datePattern value="yyyy-MM-dd'.log'"/>
			<MaxSizeRollBackups value="20"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%d{dd.MM.yyyy HH:mm:ss.fff} %property{user} [%t] %-5p %c - %m%n"/>
			</layout>
		</appender>
		<appender name="ExportInfo" type="log4net.Appender.RollingFileAppender, log4net">
			<lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
			<file value="Logs/PrgDataDebugExport_"/>
			<appendToFile value="true"/>
			<rollingStyle value="Date"/>
			<staticLogFileName value="false"/>
			<datePattern value="yyyy-MM-dd'.log'"/>
			<MaxSizeRollBackups value="20"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%d{dd.MM.yyyy HH:mm:ss.fff} %property{user} [%t] %-5p %c - %m%n"/>
			</layout>
		</appender>
		<appender name="Email" type="log4net.Appender.SmtpAppender,log4net">
			<to value="kvasovtest@analit.net"/>
			<from value="service@analit.net"/>
			<subject value="Ошибка в сервисе подготовки данных"/>
			<smtpHost value="box.analit.net"/>
			<lossy value="false"/>
			<bufferSize value="1"/>
			<threshold value="ERROR"/>
			<layout type="log4net.Layout.PatternLayout,log4net">
				<conversionPattern value="Дата: %date%nХост: %property{log4net:HostName}%nПользователь: %property{user}%n%m%n"/>
			</layout>
		</appender>
		<root>
			<level value="WARN"/>
			<appender-ref ref="Email"/>
			<appender-ref ref="LogFile"/>
		</root>
		<logger name="PrgData.PrgDataEx">
			<level value="DEBUG"/>
			<appender-ref ref="DebugLogFile"/>
		</logger>
		<logger name="PrgData.LogRequestHelper" additivity="false">
			<level value="INFO"/>
			<appender-ref ref="LogFile"/>
		</logger>
		<logger name="PrgData.Common.DebugReplicationHelper" additivity="false">
			<level value="DEBUG"/>
			<appender-ref ref="ExportInfo"/>
		</logger>
	</log4net>
</configuration>
